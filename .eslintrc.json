{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-console": "warn",
    "prefer-const": "error",
    "quotes": [
      "warn",
      "single"
    ],
    "jsx-quotes": [
      "error",
      "prefer-double"
    ],
    "indent": [
      "warn",
      2
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}